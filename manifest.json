{
	"name" : "FeedMeLinks",
	"version" : "0.1",
	"description" : "Easily consume links from your favorite feeds!",
	"icons" : {
		  "16" : "megusta_16.png",
		  "48" : "megusta_48.png",
		  "128" : "megusta_128.png"
		  },
